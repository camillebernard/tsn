@prefix change: <http://linkedearth.org/change/ns#> .

#################################################################
# Ontology TSN
#################################################################
<http://purl.org/net/tsn#> a owl:Ontology, voaf:Vocabulary;
	cc:license <http://creativecommons.org/licenses/by/3.0/>;
	dcterms:creator [ a foaf:Person; foaf:name "Camille BERNARD"; foaf:homepage <http://lig-membres.imag.fr/bernardc/>],
									[ a foaf:Person; foaf:name "Marlène VILLANOVA-OLIVER"; foaf:homepage <http://lig-membres.imag.fr/villanov/> ],
									[ a foaf:Person; foaf:name "Jérôme GENSEL"; foaf:homepage <http://lig-membres.imag.fr/gensel/> ],
									[ a foaf:Person; foaf:name "Hy DAO"; foaf:homepage <https://unige.ch/gedt/membres/dao/> ];
	dcterms:description "This RDF ontology allows describing any Territorial Statistical Nomenclature (TSN) (i.e., partition of the territory) used as a support to socio-economic data (statistical data that describe territory in terms of population, unemployement rate, transport access, etc.)."@en;
	dcterms:issued "2017-01-31"^^xsd:date;
	dcterms:modified "2017-01-31"^^xsd:date;
	dcterms:publisher <http://steamer.imag.fr>;
	dcterms:title "Territorial Statistical Nomenclature Ontology"@en;
	vann:preferredNamespacePrefix "tsn";
	vann:preferredNamespaceUri <http://purl.org/net/tsn#>;
	cc:license <http://www.data.gouv.fr/Licence-Ouverte-Open-Licence>;
	owl:versionInfo "1.0"^^xsd:decimal;
	rdfs:comment "First version of the vocabulary".

#################################################################
# Classes
#################################################################

#---------- TSN SUPER CLASSES ---------------------

tsn:TSNComponent a rdfs:Class ;
 	  rdfs:label "TSNComponent"@en ;
    rdfs:comment "The class TSNComponent is the Superclass of any Territorial Statistical Nomenclature components." .

tsn:Version a rdfs:Class ;
 	 rdfs:label "Version"@en ;
   rdfs:comment "A TSNComponent may have several Version of it, valid for a period of time. A Version resource hold all the attributs of a TSNComponent that may vary, i.e., all attributs except those that hold the identity of the resource. The Version concept is closed to the TimeSlice concept of ontologies of fluent. "@en ;
   rdfs:subClassOf tsn:TSNComponent .

tsn:TSNFeature a rdfs:Class ;
	 rdfs:label "TSNFeature"@en ;
	 rdfs:comment "A TSNComponent may be a feature (i.e., a geographic resource). The features of the TSN-Ontology are TerritoryVersion, LevelVersion and UnitVersion resources. The TSNFeature class is a subclass of the geo:Feature concept of the OGC Geosparql ontology (please consult http://www.opengis.net/ont/geosparql#Feature for a definition of this concept)"@en ;
   rdfs:subClassOf geo:Feature ;
   rdfs:subClassOf tsn:TSNComponent .

#---------- TSN CLASSES ---------------------

tsn:Territory a rdfs:Class ;
	 rdfs:label "Territory"@en ;
	 rdfs:comment "A territory is an abstract representation of a portion of geographic space that is claimed or occupied by a person or group of persons or by an institution (source http://www.oxfordbibliographies.com/view/document/obo-9780199874002/obo-9780199874002-0076.xml). The boundaries of the territory define the study area of the Territorial Statistical Nomenclature that covers the territory."@en ;
   rdfs:subClassOf tsn:TSNComponent .

tsn:TerritoryVersion a rdfs:Class ;
	 rdfs:label "TerritoryVersion"@en ;
	 rdfs:comment "A TerritoryVersion resource is a unique representation of a Territory at a determined period of time. As the boundaries of a territory vary (e.g., fusion with other territory), it results succesives versions of the territory. A territory may have as many versions as it underwent modifications. For instance, European Union of 27 and European Union of 28 member states are two versions of the European Union territory."@en ;
	 rdfs:subClassOf tsn:Version ;
	 rdfs:subClassOf tsn:TSNFeature .

tsn:Nomenclature a rdfs:Class ;
 	 rdfs:label "Nomenclature"@en ;
   rdfs:comment "A Territorial Statistical Nomenclature (i.e., Nomenclature resource) is an abstract representation of the subdivisions of a territory into Levels and Territorial Units. A Nomenclature can be seen as a hierarchy of sets of Units with at least one Level. It is designed to answer administrative, electoral and especially statistical needs. "@en ;
   rdfs:subClassOf tsn:TSNComponent .

tsn:NomenclatureVersion a rdfs:Class ;
 	 rdfs:label "NomenclatureVersion"@en ;
   rdfs:comment "A NomenclatureVersion resource is a unique representation of a Nomenclature, stable for a period of time. Stability ensures that statistical data refer to the same territorial units for a certain period of time (source http://ec.europa.eu/eurostat/web/nuts/history) (i.e., the reference period of the NomenclatureVersion)."@en ;
   rdfs:subClassOf tsn:Version .

tsn:Level a rdfs:Class ;
   rdfs:label "Level"@en ;
   rdfs:comment "A Territorial Statistical Nomenclature may define various levels of observation of the territory (e.g., region or district levels)"@en ;
   rdfs:subClassOf tsn:TSNComponent .

tsn:LevelVersion a rdfs:Class ;
	 rdfs:label "LevelVersion"@en ;
	 rdfs:comment "A LevelVersion resource is a unique representation of a Level, stable for a period of time. This period of time is the one of the NomenclatureVersion it belongs to (i.e., the reference period of the NomenclatureVersion)."@en ;
	 rdfs:subClassOf tsn:Version ;
	 rdfs:subClassOf tsn:TSNFeature .

tsn:Unit a rdfs:Class ;
  rdfs:label "Unit"@en ;
  rdfs:comment "Each level of a Territorial Statistical Nomenclature is split into Territorial Units i.e., a piece of geographic space that may contain sub-units (or not, if the unit belongs to the lowest level of the nomenclature)."@en ;
  owl:sameAs ramon:Unit ;
  rdfs:subClassOf tsn:TSNComponent .

tsn:UnitVersion a rdfs:Class ;
	rdfs:label "UnitVersion"@en ;
	rdfs:comment "A UnitVersion resource is a unique representation of a Unit, stable for a period of time. This period of time is the one of the NomenclatureVersion it belongs to (i.e., the reference period of the NomenclatureVersion)."@en ;
	rdfs:subClassOf tsn:Version ;
	rdfs:subClassOf tsn:TSNFeature .

#################################################################
# Object properties
#################################################################

# ----------------- TSNComponent properties -----------

tsn:hasIdentifier  a rdf:Property ;
   rdfs:label "hasIdentifier"@en ;
   rdfs:comment "Indicates an unambiguous reference to a TSNComponent, throughout the TSN versions. This attribut holds the identity of the TSNComponent i.e., if it changes then the TSNComponent ceases to exist."@en ;
   rdfs:domain  tsn:TSNComponent ;
   rdfs:range <http://www.w3.org/2000/01/rdf-schema#Literal> ;
   rdfs:subPropertyOf dcterms:identifier .

tsn:hasName  a rdf:Property ;
	rdfs:label "hasName"@en ;
	rdfs:comment "Indicates the Name of a TSNComponent."@en ;
	rdfs:domain  tsn:TSNComponent ;
	rdfs:range <http://www.w3.org/2000/01/rdf-schema#Literal> .

tsn:hasAcronym  a rdf:Property ;
   rdfs:label "hasAcronym"@en ;
   rdfs:comment "Indicates an Acronym of a TSNComponent (e.g., EU27 is the prefered acronym used to designate the European Union made of 27 members)."@en ;
   rdfs:domain  tsn:TSNComponent ;
   rdfs:range <http://www.w3.org/2000/01/rdf-schema#Literal> .

tsn:hasDescription  a rdf:Property ;
   rdfs:label "hasDescription"@en ;
   rdfs:comment "Indicates a Description of a TSNComponent. It may include but is not limited to: an abstract, a table of contents, a graphical representation, or a free-text account of the TSNComponent."@en ;
   rdfs:domain  tsn:TSNComponent ;
   rdfs:subPropertyOf dcterms:description .

# ----------------- Version properties -----------------

tsn:isSuccessorOf  a rdf:Property ;
   rdfs:label "isSuccessorOf"@en ;
   rdfs:comment "Indicates the successor Version of a Version resource."@en ;
   rdfs:domain  tsn:Version ;
   rdfs:range  tsn:Version ;
   owl:inverseOf  tsn:isPredecessorOf .

tsn:isPredecessorOf  a rdf:Property ;
   rdfs:label "isPredecessorOf"@en ;
   rdfs:comment "Indicates the predecessor Version of a Version resource."@en ;
   rdfs:domain  tsn:Version ;
   rdfs:range  tsn:Version ;
   owl:inverseOf  tsn:isSuccessorOf .

tsn:referencePeriod  a rdf:Property ;
   rdfs:label "referencePeriod"@en ;
   rdfs:comment "Indicates the period of time (i.e., Interval defined with two point in time <https://www.w3.org/2006/time#hasBeginning> and <https://www.w3.org/2006/time#hasEnd>) to which a Version resource  is stable and considered as the official one."@en ;
   rdfs:domain  tsn:Version ;
   rdfs:range time:Interval .

# ----------------- Feature properties -----------------

tsn:hasGeometry  a rdf:Property ;
   rdfs:label "hasGeometry"@en ;
   rdfs:comment "Indicates a Geometry of a spatial resource (i.e., TSNFeature (TerritoryVersion, LevelVersion or UnitVersion) resource)"@en ;
   rdfs:domain  tsn:TSNFeature ;
   rdfs:range geo:Geometry ;
   owl:sameAs geo:hasGeometry .

# ----------------- Versioned resources (i.e., Territory and Nomenclature) properties ----------------

tsn:hasVersion a rdf:Property ;
   rdfs:label "hasVersion"@en ;
   rdfs:comment "Indicates a resource is a Version, edition, or adaptation of the described resource."@en ;
	 rdfs:domain  tsn:TSNComponent ;
   rdfs:range tsn:Version ;
   owl:inverseOf  tsn:isVersionOf ;
   rdfs:subPropertyOf dcterms:hasVersion .


# ----------------- Version properties ----------

tsn:isVersionOf a rdf:Property ;
   rdfs:label "isVersionOf"@en ;
   rdfs:comment "Indicates a TSNComponent of which the described resource is a version, edition, or adaptation."@en ;
   rdfs:domain  tsn:Version ;
	 rdfs:range  tsn:TSNComponent ;
   owl:inverseOf  tsn:hasVersion ;
   rdfs:subPropertyOf dcterms:isVersionOf .

# ----------------- TerritoryVersion properties ----------

tsn:isCoveredBy a rdf:Property ;
	  rdfs:label "isCoveredBy"@en ;
	  rdfs:comment "Indicates a NomenclatureVersion that covers a TerritoryVersion i.e., the TerritoryVersion is the spatial applicability of the NomenclatureVersion."@en ;
	  rdfs:domain  tsn:TerritoryVersion ;
	  rdfs:range tsn:NomenclatureVersion ;
	  owl:inverseOf  tsn:covers .

# ----------------- NomenclatureVersion properties ----

tsn:hasLevel  a rdf:Property ;
   rdfs:label "hasLevel"@en ;
   rdfs:comment "Indicates a LevelVersion in the NomenclatureVersion hierarchy."@en ;
   rdfs:domain  tsn:NomenclatureVersion ;
   rdfs:range tsn:LevelVersion .

tsn:versionRights  a rdf:Property ; #consult http://rdfdata.eionet.europa.eu/page/ramon/nuts2008/
   rdfs:label "versionRights"@en ;
   rdfs:comment "Information about rights held in and over the resource. Typically, rights information includes a statement about various property rights associated with the resource, including intellectual property rights."@en ;
   rdfs:domain  tsn:NomenclatureVersion ;
   rdfs:range dcterms:rights .

tsn:nomenclatureVersionSource  a rdf:Property ; #consult http://rdfdata.eionet.europa.eu/page/ramon/nuts2008/ /!\ préférer "dcterms:source"
   rdfs:label "nomenclatureVersionSource"@en ;
   rdfs:comment "A related resource from which the described resource is derived. Example: European Commission - http://ec.europa.eu/eurostat/ramon/nuts/codelist_en.cfm?list=nuts"@en ;
   rdfs:domain  tsn:NomenclatureVersion ;
   rdfs:range dcterms:source .

tsn:covers a rdf:Property ;
	 rdfs:label "covers"@en ;
	 rdfs:comment "Indicates a TerritoryVersion covered by a NomenclatureVersion i.e., the TerritoryVersion is the spatial applicability of the NomenclatureVersion."@en ;
	 rdfs:domain  tsn:NomenclatureVersion ;
	 rdfs:range tsn:TerritoryVersion ;
	 owl:inverseOf  tsn:isCoveredBy .

# -----------------  Level properties ---------------

tsn:belongsToNomenclatureVersion a rdf:Property;
   rdfs:label "belongsToNomenclatureVersion"@en ;
   rdfs:comment "Indicates the NomenclatureVersion the described LevelVersion resource belongs to."@en ;
   rdfs:domain  tsn:LevelVersion ;
   rdfs:range tsn:NomenclatureVersion ;
   owl:inverseOf  tsn:hasLevel .

tsn:hasUpperLevel  a rdf:Property ;
   rdfs:label "hasUpperLevel"@en ;
   rdfs:comment "Indicates the LevelVersion that is immediately above the described LevelVersion."@en ;
   rdfs:domain  tsn:LevelVersion  ;
   rdfs:range  tsn:LevelVersion  ;
   owl:inverseOf  tsn:hasLowerLevel .

tsn:hasLowerLevel  a rdf:Property ;
   rdfs:label "hasLowerLevel"@en ;
   rdfs:comment "Indicates the LevelVersion that is immediately below the described LevelVersion."@en ;
   rdfs:domain  tsn:LevelVersion  ;
   rdfs:range  tsn:LevelVersion ;
   owl:inverseOf  tsn:hasUpperLevel .

tsn:hasUnit  a rdf:Property ;
   rdfs:label "hasUnit"@en ;
   rdfs:comment "Indicates a UnitVersion that belongs to the described LevelVersion."@en ;
   rdfs:domain  tsn:LevelVersion ;
   rdfs:range tsn:UnitVersion .

# ----------------- Unit properties -----------------

tsn:belongsToLevel a rdf:Property ;
   rdfs:label "belongsToLevel"@en ;
   rdfs:comment "Indicates the LevelVersion the described UnitVersion resource belongs to."@en ;
   rdfs:domain  tsn:UnitVersion ;
   rdfs:range tsn:LevelVersion ;
   owl:inverseOf  tsn:hasUnit .

tsn:hasSuperUnit  a rdf:Property ;
   rdfs:label "hasSuperUnit"@en ;
   rdfs:comment "Indicates the UnitVersion that is immediately above/contains the described UnitVersion."@en ;
   rdfs:domain  tsn:UnitVersion  ;
   rdfs:range  tsn:UnitVersion  ;
   owl:inverseOf  tsn:hasSubUnit ;
   owl:sameAs ramon:hasParentRegion .

tsn:hasSubUnit  a rdf:Property ;
   rdfs:label "hasSubUnit"@en ;
   rdfs:comment "Indicates a UnitVersion that is contained (spatially or not, as, for instance, an administrative hierarchy may be based on other criteria than spatial criteria) in the described UnitVersion."@en ;
   rdfs:domain  tsn:UnitVersion  ;
   rdfs:range  tsn:UnitVersion ;
   owl:sameAs ramon:hasSubRegion .

# end TSN-Ontology
