<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
   xmlns:cc="http://creativecommons.org/ns#"
   xmlns:dcterms="http://purl.org/dc/terms/"
   xmlns:foaf="http://xmlns.com/foaf/0.1/"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:vann="http://purl.org/vocab/vann/"
>
  <rdf:Description rdf:about="http://purl.org/net/tsn#LevelVersion">
    <rdfs:subClassOf rdf:resource="http://purl.org/net/tsn#Version"/>
    <rdfs:label xml:lang="en">LevelVersion</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:comment xml:lang="en">A LevelVersion resource is a unique representation of a Level, stable for a period of time. This period of time is the one of the NomenclatureVersion it belongs to (i.e., the reference period of the NomenclatureVersion).</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#covers">
    <rdfs:comment xml:lang="en">Indicates a TerritoryVersion covered by a NomenclatureVersion i.e., the TerritoryVersion is the spatial applicability of the NomenclatureVersion.</rdfs:comment>
    <rdfs:label xml:lang="en">covers</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/net/tsn#TerritoryVersion"/>
    <owl:inverseOf rdf:resource="http://purl.org/net/tsn#isCoveredBy"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#NomenclatureVersion"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#hasSuperUnit">
    <owl:inverseOf rdf:resource="http://purl.org/net/tsn#hasSubUnit"/>
    <owl:sameAs rdf:resource="http://ec.europa.eu/eurostat/ramon/ontologies/geographic.rdf#hasParentRegion"/>
    <rdfs:label xml:lang="en">hasSuperUnit</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#UnitVersion"/>
    <rdfs:comment xml:lang="en">Indicates the UnitVersion that is immediately above/contains the described UnitVersion.</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/net/tsn#UnitVersion"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#hasIdentifier">
    <rdfs:label xml:lang="en">hasIdentifier</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    <rdfs:comment xml:lang="en">Indicates an unambiguous reference to a TSNComponent, throughout the TSN versions. This attribut holds the identity of the TSNComponent i.e., if it changes then the TSNComponent ceases to exist.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#TSNComponent"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#hasUpperLevel">
    <rdfs:comment xml:lang="en">Indicates the LevelVersion that is immediately above the described LevelVersion.</rdfs:comment>
    <owl:inverseOf rdf:resource="http://purl.org/net/tsn#hasLowerLevel"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">hasUpperLevel</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/net/tsn#LevelVersion"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#LevelVersion"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#TSNFeature">
    <rdfs:label xml:lang="en">TSNFeature</rdfs:label>
    <rdfs:comment xml:lang="en">A TSNComponent may be a feature (i.e., a geographic resource). The features of the TSN-Ontology are TerritoryVersion and UnitVersion resources. The TSNFeature class is a subclass of the geo:Feature concept of the OGC Geosparql ontology (please consult http://www.opengis.net/ont/geosparql#Feature for a definition of this concept)</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/tsn#TSNComponent"/>
    <rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#Nomenclature">
    <rdfs:label xml:lang="en">Nomenclature</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:comment xml:lang="en">A Territorial Statistical Nomenclature (i.e., Nomenclature resource) is an abstract representation of the subdivisions of a territory into Levels and Territorial Units. A Nomenclature can be seen as a hierarchy of sets of Units with at least one Level. It is designed to answer administrative, electoral and especially statistical needs. </rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/tsn#TSNComponent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#hasGeometry">
    <rdfs:comment xml:lang="en">Indicates a Geometry of a spatial resource (i.e., TSNFeature (TerritoryVersion or UnitVersion) resource)</rdfs:comment>
    <owl:sameAs rdf:resource="http://www.opengis.net/ont/geosparql#hasGeometry"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#TSNFeature"/>
    <rdfs:range rdf:resource="http://www.opengis.net/ont/geosparql#Geometry"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">hasGeometry</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#hasUnit">
    <rdfs:range rdf:resource="http://purl.org/net/tsn#UnitVersion"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#LevelVersion"/>
    <rdfs:label xml:lang="en">hasUnit</rdfs:label>
    <rdfs:comment xml:lang="en">Indicates a UnitVersion that belongs to the described LevelVersion.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#TSNComponent">
    <rdfs:label xml:lang="en">TSNComponent</rdfs:label>
    <rdfs:comment>The class TSNComponent is the Superclass of any Territorial Statistical Nomenclature components.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#hasLevel">
    <rdfs:range rdf:resource="http://purl.org/net/tsn#LevelVersion"/>
    <rdfs:label xml:lang="en">hasLevel</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#NomenclatureVersion"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:comment xml:lang="en">Indicates a LevelVersion in the NomenclatureVersion hierarchy.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:nodeID="ub2bL22C10">
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <foaf:homepage rdf:resource="http://lig-membres.imag.fr/gensel/"/>
    <foaf:name>Jérôme GENSEL</foaf:name>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#hasVersion">
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#TSNComponent"/>
    <rdfs:label xml:lang="en">hasVersion</rdfs:label>
    <owl:inverseOf rdf:resource="http://purl.org/net/tsn#isVersionOf"/>
    <rdfs:range rdf:resource="http://purl.org/net/tsn#Version"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/hasVersion"/>
    <rdfs:comment xml:lang="en">Indicates a resource is a Version, edition, or adaptation of the described resource.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#NomenclatureVersion">
    <rdfs:comment xml:lang="en">A NomenclatureVersion resource is a unique representation of a Nomenclature, stable for a period of time. Stability ensures that statistical data refer to the same territorial units for a certain period of time (source http://ec.europa.eu/eurostat/web/nuts/history) (i.e., the reference period of the NomenclatureVersion).</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">NomenclatureVersion</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/tsn#Version"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#">
    <cc:license rdf:resource="http://www.data.gouv.fr/Licence-Ouverte-Open-Licence"/>
    <rdf:type rdf:resource="http://purl.org/vocommons/voaf#Vocabulary"/>
    <dcterms:creator rdf:nodeID="ub2bL23C10"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.0</owl:versionInfo>
    <dcterms:publisher rdf:resource="http://steamer.imag.fr"/>
    <vann:preferredNamespaceUri rdf:resource="http://purl.org/net/tsn#"/>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-01-31</dcterms:modified>
    <vann:preferredNamespacePrefix>tsn</vann:preferredNamespacePrefix>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-01-31</dcterms:issued>
    <cc:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
    <dcterms:creator rdf:nodeID="ub2bL21C10"/>
    <dcterms:creator rdf:nodeID="ub2bL20C18"/>
    <dcterms:description xml:lang="en">This RDF ontology allows describing any Territorial Statistical Nomenclature (TSN) (i.e., partition of the territory) used as a support to socio-economic data (statistical data that describe territory in terms of population, unemployement rate, transport access, etc.).</dcterms:description>
    <rdfs:comment>First version of the vocabulary</rdfs:comment>
    <dcterms:creator rdf:nodeID="ub2bL22C10"/>
    <dcterms:title xml:lang="en">Territorial Statistical Nomenclature Ontology</dcterms:title>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#hasDescription">
    <rdfs:label xml:lang="en">hasDescription</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#TSNComponent"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:comment xml:lang="en">Indicates a Description of a TSNComponent. It may include but is not limited to: an abstract, a table of contents, a graphical representation, or a free-text account of the TSNComponent.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/description"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#nomenclatureVersionSource">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">nomenclatureVersionSource</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#NomenclatureVersion"/>
    <rdfs:comment xml:lang="en">A related resource from which the described resource is derived. Example: European Commission - http://ec.europa.eu/eurostat/ramon/nuts/codelist_en.cfm?list=nuts</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/dc/terms/source"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#belongsToLevel">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:range rdf:resource="http://purl.org/net/tsn#LevelVersion"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#UnitVersion"/>
    <rdfs:label xml:lang="en">belongsToLevel</rdfs:label>
    <owl:inverseOf rdf:resource="http://purl.org/net/tsn#hasUnit"/>
    <rdfs:comment xml:lang="en">Indicates the LevelVersion the described UnitVersion resource belongs to.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#isPredecessorOf">
    <rdfs:range rdf:resource="http://purl.org/net/tsn#Version"/>
    <rdfs:comment xml:lang="en">Indicates the predecessor Version of a Version resource.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">isPredecessorOf</rdfs:label>
    <owl:inverseOf rdf:resource="http://purl.org/net/tsn#isSuccessorOf"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#Version"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#Level">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Level</rdfs:label>
    <rdfs:comment xml:lang="en">A Territorial Statistical Nomenclature may define various levels of observation of the territory (e.g., region or district levels)</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/tsn#TSNComponent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#TerritoryVersion">
    <rdfs:subClassOf rdf:resource="http://purl.org/net/tsn#TSNFeature"/>
    <rdfs:comment xml:lang="en">A TerritoryVersion resource is a unique representation of a Territory at a determined period of time. As the boundaries of a territory vary (e.g., fusion with other territory), it results succesives versions of the territory. A territory may have as many versions as it underwent modifications. For instance, European Union of 27 and European Union of 28 member states are two versions of the European Union territory.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/tsn#Version"/>
    <rdfs:label xml:lang="en">TerritoryVersion</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="ub2bL20C18">
    <foaf:name>Camille BERNARD</foaf:name>
    <foaf:homepage rdf:resource="http://lig-membres.imag.fr/bernardc/"/>
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#UnitVersion">
    <rdfs:subClassOf rdf:resource="http://purl.org/net/tsn#Version"/>
    <rdfs:comment xml:lang="en">A UnitVersion resource is a unique representation of a Unit, stable for a period of time. This period of time is the one of the NomenclatureVersion it belongs to (i.e., the reference period of the NomenclatureVersion).</rdfs:comment>
    <rdfs:label xml:lang="en">UnitVersion</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#versionRights">
    <rdfs:label xml:lang="en">versionRights</rdfs:label>
    <rdfs:comment xml:lang="en">Information about rights held in and over the resource. Typically, rights information includes a statement about various property rights associated with the resource, including intellectual property rights.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#NomenclatureVersion"/>
    <rdfs:range rdf:resource="http://purl.org/dc/terms/rights"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#isCoveredBy">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <owl:inverseOf rdf:resource="http://purl.org/net/tsn#covers"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#TerritoryVersion"/>
    <rdfs:comment xml:lang="en">Indicates a NomenclatureVersion that covers a TerritoryVersion i.e., the TerritoryVersion is the spatial applicability of the NomenclatureVersion.</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/net/tsn#NomenclatureVersion"/>
    <rdfs:label xml:lang="en">isCoveredBy</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#referencePeriod">
    <rdfs:range rdf:resource="https://www.w3.org/2006/time#Interval"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#Version"/>
    <rdfs:label xml:lang="en">referencePeriod</rdfs:label>
    <rdfs:comment xml:lang="en">Indicates the period of time (i.e., Interval defined with two point in time &lt;https://www.w3.org/2006/time#hasBeginning&gt; and &lt;https://www.w3.org/2006/time#hasEnd&gt;) to which a Version resource  is stable and considered as the official one.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#Version">
    <rdfs:label xml:lang="en">Version</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:comment xml:lang="en">A TSNComponent may have several Version of it, valid for a period of time. A Version resource hold all the attributs of a TSNComponent that may vary, i.e., all attributs except those that hold the identity of the resource. The Version concept is closed to the TimeSlice concept of ontologies of fluent. </rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/tsn#TSNComponent"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#hasSubUnit">
    <rdfs:label xml:lang="en">hasSubUnit</rdfs:label>
    <rdfs:comment xml:lang="en">Indicates a UnitVersion that is contained (spatially or not, as, for instance, an administrative hierarchy may be based on other criteria than spatial criteria) in the described UnitVersion.</rdfs:comment>
    <owl:sameAs rdf:resource="http://ec.europa.eu/eurostat/ramon/ontologies/geographic.rdf#hasSubRegion"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:range rdf:resource="http://purl.org/net/tsn#UnitVersion"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#UnitVersion"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#belongsToNomenclatureVersion">
    <rdfs:range rdf:resource="http://purl.org/net/tsn#NomenclatureVersion"/>
    <owl:inverseOf rdf:resource="http://purl.org/net/tsn#hasLevel"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:comment xml:lang="en">Indicates the NomenclatureVersion the described LevelVersion resource belongs to.</rdfs:comment>
    <rdfs:label xml:lang="en">belongsToNomenclatureVersion</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#LevelVersion"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#hasAcronym">
    <rdfs:comment xml:lang="en">Indicates an Acronym of a TSNComponent (e.g., EU27 is the prefered acronym used to designate the European Union made of 27 members).</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#TSNComponent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:label xml:lang="en">hasAcronym</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#Unit">
    <rdfs:subClassOf rdf:resource="http://purl.org/net/tsn#TSNFeature"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <owl:sameAs rdf:resource="http://ec.europa.eu/eurostat/ramon/ontologies/geographic.rdf#Unit"/>
    <rdfs:label xml:lang="en">Unit</rdfs:label>
    <rdfs:comment xml:lang="en">Each level of a Territorial Statistical Nomenclature is split into Territorial Units i.e., a piece of geographic space that may contain sub-units (or not, if the unit belongs to the lowest level of the nomenclature).</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:nodeID="ub2bL23C10">
    <foaf:homepage rdf:resource="https://unige.ch/gedt/membres/dao/"/>
    <foaf:name>Hy DAO</foaf:name>
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#Territory">
    <rdfs:comment xml:lang="en">A territory is an abstract representation of a portion of geographic space that is claimed or occupied by a person or group of persons or by an institution (source http://www.oxfordbibliographies.com/view/document/obo-9780199874002/obo-9780199874002-0076.xml). The boundaries of the territory define the study area of the Territorial Statistical Nomenclature that covers the territory.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/tsn#TSNComponent"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Territory</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#isVersionOf">
    <owl:inverseOf rdf:resource="http://purl.org/net/tsn#hasVersion"/>
    <rdfs:label xml:lang="en">isVersionOf</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/net/tsn#TSNComponent"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/isVersionOf"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#Version"/>
    <rdfs:comment xml:lang="en">Indicates a TSNComponent of which the described resource is a version, edition, or adaptation.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#isSuccessorOf">
    <owl:inverseOf rdf:resource="http://purl.org/net/tsn#isPredecessorOf"/>
    <rdfs:range rdf:resource="http://purl.org/net/tsn#Version"/>
    <rdfs:comment xml:lang="en">Indicates the successor Version of a Version resource.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#Version"/>
    <rdfs:label xml:lang="en">isSuccessorOf</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="ub2bL21C10">
    <foaf:name>Marlène VILLANOVA-OLIVER</foaf:name>
    <foaf:homepage rdf:resource="http://lig-membres.imag.fr/villanov/"/>
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/net/tsn#hasName">
    <rdfs:domain rdf:resource="http://purl.org/net/tsn#TSNComponent"/>
    <rdfs:comment xml:lang="en">Indicates the Name of a TSNComponent.</rdfs:comment>
    <rdfs:label xml:lang="en">hasName</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
